{
  "name": "mimmi20/mezzio-router-laminasrouter-factory",
  "description": "Provides a Factory to set a TranslatorAwareTreeRouteStack for a laminas-router in a Mezzio Application",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/mezzio-router-laminasrouter-factory/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-router-laminasrouter-factory",
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-router-laminasrouter-factory/issues",
    "source": "https://github.com/mimmi20/mezzio-router-laminasrouter-factory"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-i18n": "^2.31.0",
    "laminas/laminas-router": "^3.16.0",
    "laminas/laminas-translator": "^1.2.0",
    "mezzio/mezzio-laminasrouter": "^3.12.0",
    "mezzio/mezzio-router": "^4.1.0",
    "psr/container": "^1.1.2 || ^2.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.31.9",
    "mimmi20/coding-standard": "^6.2.5",
    "nikic/php-parser": "^5.7.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.33",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.5.2",
    "rector/rector": "^2.2.14",
    "rector/type-perfect": "^2.1.1",
    "shipmonk/composer-dependency-analyser": "^1.8.4",
    "symfony/process": "^7.3.4",
    "symplify/phpstan-rules": "^14.9.5",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.1.0",
    "tomasvotruba/unused-public": "^2.1.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\Mezzio\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Mezzio\\Router\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\Mezzio\\Router\\ConfigProvider"
    }
  }
}
